### Generics
type ListOf<T>
  items: List<T>

let list = ListOf<String>(
  items = ["a", "b", "c"]
)

let tuple<T, U> = (first: T, second: U) => (first, second)

let merge <T> = (l1, l2: List<T>) => List<T>
  return [...l1, ...l2]

# The keywords where + satisfies are the keys to to create complex generics types

let makeEat<T> where T satisfies Eater = (animal: T) 
  animal.eat()

let makeEatAndSleep<T, U> where (
  T satisfies Eater
  T satisfies Eater
) = (t: T, u: U)
  t.eat()
  u.sleep()
