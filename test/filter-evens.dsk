let filter_evens: fn = (arr: []number) -> []number
    let mut result : []number = []
    let n: number = len(arr)
    let mut i: number = 0

    for i < n
        let val: number = arr[i]
        if val % 2 == 0
            append(result, val)
        i = i + 1

    return result

let arr: []number = [1,3,5,1,10,33,34,33,100,20,10,40]
let evens: []number = filter_evens(arr)
echo(evens)

