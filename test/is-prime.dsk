let isPrime: fn = (n: number) -> bool
    if n < 2 
        return false

    let mut i: number = 2
    for i * i <= n
        if n % i == 0
            return false
        i = i + 1

    return true

echo(isPrime(1))
echo(isPrime(3))
echo(isPrime(5))
echo(isPrime(7))
