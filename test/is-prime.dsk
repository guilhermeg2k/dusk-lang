let isPrime: fn = fn(n: number) -> bool
    if n < 2 
        return false

    let mut i: number = 2
    let mut r: number = i * i

    for r <= n
        let comp: number = n % i
        if comp == 0
            return false

        i = i + 1
        r = i * i

    return true

echo(isPrime(1))
echo(isPrime(3))
echo(isPrime(5))
echo(isPrime(7))
echo(isPrime(9))
echo(isPrime(18))
