let bubble_sort: fn = (mut arr: []number) -> void
    let n: number = len(arr)
    let mut i: number = 0

    for i < n
        let mut j: number = 0

        for j < n - 1
            let aux: number = arr[j]
            if arr[j] > arr[j + 1]
                arr[j] = arr[j + 1]
                arr[j + 1] = aux
            j = j + 1

        i = i + 1

let mut v: []number = [1,2,51,22, 100, 500, 321, 21312, 21312]
bubble_sort(v)
echo(v)
